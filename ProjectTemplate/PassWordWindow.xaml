<Window x:Class="ProjectTemplate.PassWordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTemplate"
        mc:Ignorable="d"
        Title="PassWordWindow" Height="150" Width="200"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <StackPanel>
            <PasswordBox x:Name="PassWordInput" Width="160" Margin="10"></PasswordBox>

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Click="OK_Press" Width="80" Margin="5">OK</Button>
                <Button Click="Cancel_Press" Width="80" Margin="5">Cancel</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
